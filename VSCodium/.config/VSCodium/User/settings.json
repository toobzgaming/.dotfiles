{
  "editor.fontFamily": "'FiraCode Nerd Font'",
  "editor.fontLigatures": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "workbench.colorTheme": "Dracula Theme",
  "cSpell.customDictionaries": {
    "custom-dictionary-user": {
      "name": "custom-dictionary-user",
      "path": "~/.cspell/custom-dictionary-user.txt",
      "addWords": true,
      "scope": "user"
    }
  },
  "editor.minimap.enabled": false,
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showClasses": false,
  "breadcrumbs.showConstants": false,
  "breadcrumbs.showConstructors": false,
  "breadcrumbs.showEnumMembers": false,
  "breadcrumbs.showEnums": false,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showFields": false,
  "breadcrumbs.showFiles": false,
  "breadcrumbs.showFunctions": false,
  "breadcrumbs.showInterfaces": false,
  "breadcrumbs.showKeys": false,
  "breadcrumbs.showMethods": false,
  "breadcrumbs.showModules": false,
  "breadcrumbs.showNamespaces": false,
  "breadcrumbs.showNull": false,
  "breadcrumbs.showNumbers": false,
  "breadcrumbs.showObjects": false,
  "breadcrumbs.showOperators": false,
  "breadcrumbs.showPackages": false,
  "breadcrumbs.showProperties": false,
  "breadcrumbs.showStrings": false,
  "breadcrumbs.showStructs": false,
  "breadcrumbs.showTypeParameters": false,
  "breadcrumbs.showVariables": false,
  "breadcrumbs.symbolPath": "off",
  "editor.lineNumbers": "relative",
  "editor.glyphMargin": true,
  "editor.formatOnSave": true,
  "editor.wordWrap": "on",
  "vetur.validation.template": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.linux": "/$HOME/.config/nvim/init.lua",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "editor.rulers": [80],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "indentRainbow.indicatorStyle": "light",
  "editor.guides.bracketPairs": "active",
  "git.confirmSync": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "tailwind-fold.foldStyle": "QUOTES",
  "tailwind-fold.showTailwindImage": false,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "editor.stickyTabStops": true,
  "inlineFold.unfoldOnLineSelect": true,
  "inlineFold.unfoldedOpacity": 1,
  "whichkey.bindingOverrides": [
    {
      "keys": ";",
      "name": "+Personal",
      "type": "bindings",
      "bindings": [
        {
          "key": "o",
          "name": "Outline",
          "type": "command",
          "command": "outline.focus"
        },
        {
          "key": "h",
          "name": "History",
          "type": "command",
          "command": "timeline.focus"
        },
        {
          "key": "t",
          "name": "Tree",
          "type": "command",
          "command": "workbench.view.explorer"
        },
        {
          "key": "n",
          "name": "Neovim Toggle",
          "type": "command",
          "command": "vscode-neovim.toggle"
        },
        {
          "key": "a",
          "name": "Right Bar Toggle",
          "type": "command",
          "command": "workbench.action.toggleAuxiliaryBar"
        },

        {
          "key": "b",
          "name": "Left Bar Toggle",
          "type": "command",
          "command": "workbench.action.toggleSidebarVisibility"
        },
        {
          "key": "s",
          "name": "Search",
          "type": "command",
          "command": "workbench.action.findInFiles"
        },
        {
          "key": "r",
          "name": "Replace",
          "type": "command",
          "command": "workbench.action.replaceInFiles"
        },
        {
          "key": "g",
          "name": "Git",
          "type": "command",
          "command": "workbench.view.scm"
        }
      ]
    },
    {
      "keys": "S.O",
      "name": "Show outline",
      "type": "command",
      "command": "outline.focus"
    },
    {
      "keys": "S.h",
      "name": "Show history",
      "type": "command",
      "command": "timeline.focus"
    },
    {
      "keys": "t.n",
      "name": "Neovim",
      "type": "command",
      "command": "vscode-neovim.toggle"
    },
    {
      "keys": "T.a",
      "name": "Toggle secondary side bar visibility",
      "type": "command",
      "command": "workbench.action.toggleAuxiliaryBar"
    },
    {
      "keys": "T.S",
      "name": "Toggle status bar visibility",
      "type": "command",
      "command": "workbench.action.toggleStatusbarVisibility"
    },
    {
      "keys": "T.M",
      "name": "Toggle tom menu bar visibility",
      "type": "command",
      "command": "workbench.action.toggleMenuBar"
    },
    {
      "keys": "e.a",
      "name": "Quick Fix Action",
      "type": "command",
      "command": "editor.action.quickFix"
    }
  ],
  "editor.cursorSurroundingLines": 10,
  "git.openRepositoryInParentFolders": "never",
  "workbench.editor.showTabs": "single",
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreview": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "window.menuBarVisibility": "toggle",
  "workbench.editorAssociations": {
    "*.pdf": "default"
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "vsicons.dontShowNewVersionMessage": true,
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "editor.inlineSuggest.suppressSuggestions": true,
  "vscode-neovim.highlightGroups.highlights": {
    "IncSearch": {
      "backgroundColor": "theme.editor.findMatchBackground",
      "borderColor": "theme.editor.findMatchBorder"
    },
    "CurSearch": {
      "backgroundColor": "theme.editor.findMatchBackground",
      "borderColor": "theme.editor.findMatchBorder"
    },
    "Search": {
      "backgroundColor": "theme.editor.findMatchHighlightBackground",
      "borderColor": "theme.editor.findMatchHighlightBorder"
    }
  },
  "workbench.activityBar.location": "hidden",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.stickyScroll.maxLineCount": 3,
  "editor.stickyScroll.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.minimap.renderCharacters": false,
  "godotTools.editorPath.godot4": "/var/lib/flatpak/exports/bin/org.godotengine.Godot",
  "godot.csharp.executablePath": "/var/lib/flatpak/exports/bin/org.godotengine.GodotSharp",
  "workbench.statusBar.visible": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.folding": false,
  "editor.smoothScrolling": true,
  "breadcrumbs.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "breadcrumbs.icons": false
}
